 <Worksheet ss:Name="Calculator">
  <Table ss:ExpandedColumnCount="5" ss:ExpandedRowCount="20" x:FullColumns="1"
   x:FullRows="1" ss:DefaultRowHeight="15">
   <Column ss:AutoFitWidth="0" ss:Width="180"/>
   <Column ss:AutoFitWidth="0" ss:Width="120"/>
   <Column ss:AutoFitWidth="0" ss:Width="120"/>
   <Row ss:AutoFitHeight="0" ss:Height="25">
    <Cell ss:StyleID="sTitle"><Data ss:Type="String">Live Position Size Calculator</Data></Cell>
   </Row>
   <Row ss:Index="3">
    <Cell ss:StyleID="sHeader"><Data ss:Type="String">Parameter</Data></Cell>
    <Cell ss:StyleID="sHeader"><Data ss:Type="String">Value</Data></Cell>
    <Cell ss:StyleID="sHeader"><Data ss:Type="String">Notes</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">Account Risk ($)</Data></Cell>
    <Cell ss:StyleID="sInput"><Data ss:Type="Number">200</Data></Cell>
    <Cell><Data ss:Type="String">Max loss per trade</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">Stop Distance (Points)</Data></Cell>
    <Cell ss:StyleID="sInput"><Data ss:Type="Number">4.5</Data></Cell>
    <Cell><Data ss:Type="String">Current trade SL distance</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">Point Value ($)</Data></Cell>
    <Cell ss:StyleID="sInput"><Data ss:Type="Number">5</Data></Cell>
    <Cell><Data ss:Type="String">MES=5, ES=50, MGC=10</Data></Cell>
   </Row>
   <Row ss:Height="20">
    <Cell ss:StyleID="sMetric"><Data ss:Type="String">Total Contracts to Buy</Data></Cell>
    <Cell ss:StyleID="sCalculation" ss:Formula="=INT(R[-3]C/(R[-2]C*R[-1]C))"><Data ss:Type="Number">8</Data></Cell>
    <Cell><Data ss:Type="String">Calculated Automatically</Data></Cell>
   </Row>
   <Row ss:Index="9">
    <Cell ss:StyleID="sSubHeader"><Data ss:Type="String">TARGET BREAKDOWN</Data></Cell>
    <Cell ss:StyleID="sSubHeader"><Data ss:Type="String">Contracts</Data></Cell>
    <Cell ss:StyleID="sSubHeader"><Data ss:Type="String">Split %</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">T1 (Quick Scalp)</Data></Cell>
    <Cell ss:StyleID="sCalculation" ss:Formula="=INT(R7C2*0.2)"><Data ss:Type="Number">1</Data></Cell>
    <Cell><Data ss:Type="String">20%</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">T2 (Medium)</Data></Cell>
    <Cell ss:StyleID="sCalculation" ss:Formula="=INT(R7C2*0.3)"><Data ss:Type="Number">2</Data></Cell>
    <Cell><Data ss:Type="String">30%</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">T3 (Extended)</Data></Cell>
    <Cell ss:StyleID="sCalculation" ss:Formula="=INT(R7C2*0.3)"><Data ss:Type="Number">2</Data></Cell>
    <Cell><Data ss:Type="String">30%</Data></Cell>
   </Row>
   <Row>
    <Cell><Data ss:Type="String">T4 (Runner)</Data></Cell>
    <Cell ss:StyleID="sCalculation" ss:Formula="=R7C2-R10C2-R11C2-R12C2"><Data ss:Type="Number">3</Data></Cell>
    <Cell><Data ss:Type="String">Remainder</Data></Cell>
   </Row>
  </Table>
 </Worksheet>

